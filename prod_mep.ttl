        rdfs:label      "Damper"@en ,
                        "Spjæld"@da .
product:DuctTransition
        a owl:Class ;
        rdfs:subClassOf product:Product ;
        rdfs:label      "Duct Transition"@en ,
                        "Kanalovergange"@da .


