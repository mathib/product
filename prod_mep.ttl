#################################
# CONSTRAINTS
#################################
# Disjunctive classes
[] a owl:AllDisjointClasses ;
        owl:members
              ( product:Valve
                product:Fitting
                product:Pump
                product:Meter
                product:Coil
                product:Humidifier
                product:HeatExchanger
                product:Damper
                product:DuctTransition
                product:Duct
                product:Pipe ) .
